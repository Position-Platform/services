{
    "name": "position/services",
    "type": "project",
    "description": "Position Services",
    "keywords": [
        "framework",
        "laravel",
        "position",
        "api",
        "services",
        "firebase"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "althinect/filament-spatie-roles-permissions": "^2.0",
        "amendozaaguiar/filament-route-statistics": "^1.4",
        "awcodes/overlook": "^2.0",
        "bilfeldt/laravel-route-statistics": "^2.2",
        "doctrine/dbal": "^3.6",
        "ejarnutowski/laravel-api-key": "^1.0",
        "filament/filament": "3.0-stable",
        "guzzlehttp/guzzle": "^7.4",
        "http-interop/http-factory-guzzle": "^1.2",
        "kreait/laravel-firebase": "^5.2",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.0",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.1",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravel/vonage-notification-channel": "^3.0",
        "meilisearch/meilisearch-php": "^1.3",
        "njxqlus/filament-progressbar": "^1.0",
        "psr/simple-cache": "2.0",
        "pxlrbt/filament-excel": "^2.1",
        "shuvroroy/filament-spatie-laravel-backup": "^2.0",
        "shuvroroy/filament-spatie-laravel-health": "^2.0",
        "spatie/laravel-backup": "^8.3",
        "spatie/laravel-health": "^1.23",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^4.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
